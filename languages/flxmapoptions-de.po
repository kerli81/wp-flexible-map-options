#: ../page.optionpage.php:77
msgid "Hide the map scale"
msgstr "Karten-Skala verstecken"

#: ../page.optionpage.php:78
msgid "Enable zoom with mouse scroll wheel"
msgstr "Zoomen mit Mausrad erlauben"

#: ../page.optionpage.php:79
msgid "Enable dragging to pan"
msgstr "Verschieben der Karte per Mausklick erlauben"

#: ../page.optionpage.php:80
msgid "Enable double-clicking to zoom"
msgstr "Zoomen mit Doppelklick erlauben"

#: ../page.optionpage.php:81
msgid "show directions link in text bubble"
msgstr "Zeige Routen-Link in Textblase"

#: ../page.optionpage.php:82
msgid "allow directions to be draggable"
msgstr "Verschieben der Routen-Karte per Mausklick erlauben"

#: ../page.optionpage.php:83
msgid "suppress start and end markers when showing directions"
msgstr "Start- und End-Marker bei Routen unterdrücken"

#: ../page.optionpage.php:84
msgid "show or suppress directions steps when showing directions"
msgstr "Anzeigen der Schritte bei Routen"

#: ../page.optionpage.php:85
