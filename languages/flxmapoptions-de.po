#: ../page.optionpage.php:66
msgid "Parameters for all maps"
msgstr "Parameter für alle Karten"

#: ../page.optionpage.php:69
msgid "Width in Pixel/Percent"
msgstr "Breite in Pixel/Prozent"

#: ../page.optionpage.php:70
msgid "Height in Pixel/Percent"
msgstr "Höhe in Pixel/Prozent"

#: ../page.optionpage.php:71
msgid "Type of map to show"
msgstr "anzuzeigender Kartentyp"

#: ../page.optionpage.php:72
msgid "Hide the map type controls"
msgstr "Kartentyp-Element verstecken"

#: ../page.optionpage.php:73
msgid "Hide the panning controls"
msgstr "Navigation-Element verstecken"

#: ../page.optionpage.php:74
msgid "Hide the zoom controls"
msgstr "Zoom-Element verstecken"

#: ../page.optionpage.php:75
msgid "Hide the street view control"
msgstr "Street View-Element verstecken"

#: ../page.optionpage.php:76
msgid "Which zoom control style"
